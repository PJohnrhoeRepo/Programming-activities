	<h1 id="result" style="padding-left: 80px;"></h1>





<script type="text/javascript">
		var requirements = {
			firstname:"",
			lastname:"",
			number:"",
			address:"",
			cities:"",
			quantity:"",

			getvalues: function () {
				this.firstname = document.getElementById("fn").value;
				this.lastname = document.getElementById("ln").value;
				this.number = Number(document.getElementById("numb").value);
				if (isNaN(this.number))
        		{
          		alert("Contact # not a number");
          		document.getElementById("result").innerHTML = "";
       			}
        		else
       			{

        		}

				var addressindex=document.getElementById("address").selectedIndex;
				var selectaddress=document.getElementById("address").options;
				this.address=selectaddress[addressindex].value;

				var cityindex=document.getElementById("city").selectedIndex;
				var selectcity=document.getElementById("city").options;
				this.cities=selectcity[cityindex].value;

				var strOP = document.getElementById('strand').options;
 				var strINDEX = document.getElementById('strand').selectedIndex;
 				this.quantity = strOP[strINDEX].value;

			},
			result: function() {
				this.getvalues();
				document.getElementById("result").innerHTML =  "We will be processing your order Mr/Ms. " + this.firstname + " " + this.lastname + " " + "we will text you at this contact # " + this.number +" to ensure your order. After we processed your order of this product at this quantity of " + this.quantity + ", we will delivered it at " + this.address + " " + this.cities + ". Thank you!";
			}
		}

		
	</script>
